            <CheatEntry>
              <ID>1337334239</ID>
              <Description>"坐标式飞穿"</Description>
              <Options moAlwaysHideChildren="1" moActivateChildrenAsWell="1" moDeactivateChildrenAsWell="1"/>
              <Color>00FF00</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>{$lua}
t = getAddressList()
XM = t.getMemoryRecordByDescription("动量X")
YM = t.getMemoryRecordByDescription("动量Y")
ZM = t.getMemoryRecordByDescription("动量Z")
XR = t.getMemoryRecordByDescription('FovXZ')
YR = t.getMemoryRecordByDescription('FovY')
FlySpeed = 1.5
[ENABLE]
tpauraoo = createTimer()
tpauraoo.Interval = 1
tpauraoo.OnTimer = function(tpauraoo)
XM.value = 0
ZM.value = 0
YM.value = 0
if isKeyPressed(VK_W) then
tempXR = XR.value + math.pi
XM.value = XM.value + (math.sin(tempXR) * FlySpeed)
ZM.value = ZM.value + (math.cos(tempXR) * FlySpeed)
end
if isKeyPressed(VK_S) then
tempXR = XR.value + math.pi
XM.value = XM.value - (math.sin(tempXR) * FlySpeed)
ZM.value = ZM.value - (math.cos(tempXR) * FlySpeed)
end
if isKeyPressed(VK_A) then
tempXR = XR.value + math.pi - 1.5707963
XM.value = XM.value - (math.sin(tempXR) * FlySpeed)
ZM.value = ZM.value - (math.cos(tempXR) * FlySpeed)
end
if isKeyPressed(VK_D) then
tempXR = XR.value + math.pi + 1.5707963
XM.value = XM.value - (math.sin(tempXR) * FlySpeed)
ZM.value = ZM.value - (math.cos(tempXR) * FlySpeed)
end
if isKeyPressed(VK_SPACE) then
YM.value = 1
end
if isKeyPressed(VK_SHIFT) then
YM.value = -1
end
end
[DISABLE]
tpauraoo.destroy()
</AssemblerScript>
